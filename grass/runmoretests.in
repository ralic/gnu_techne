# Run 7: No rasterization, 4 seeds / cluster
${bindir}/grassdemo -p -Ocontext=4.2 -Ofullscreen -Otimed -Odontrasterize -Oclustering=4 -e 'require"dynamics".interval=1/24' -Operiodicprofile=./run_C4.d -Oprofilingperiod=24 -Oprofile=tags.techne.latency -Oprofile=tags.graphics.latency -Oprofile=tags.vegetation.core -Oprofile=tags.vegetation.graphics

